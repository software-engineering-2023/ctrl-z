    <div class="banker-container">
      <div class="banker-container1">
        <h1 class="banker-text">
          <span>Send Notifications and Bank</span>
          <br />
        </h1>
        <button class="banker-button button" onclick="showAlert()">
          <span class="banker-text03">
            <span>Send</span>
            <br />
          </span>
        </button>
        <input
          type="text"
          required
          autofocus
          minlength="4"
          placeholder="Enter a message"
          class="banker-textinput input"
        />
        <input
          type="text"
          required
          autofocus
          minlength="4"
          placeholder="Enter the client ID"
          class="banker-textinput1 input"
        />
        <label class="banker-text06">
          Client ID (in case of a Notification) : 
        </label>
        <select class="banker-select">
          <option value="Option 1" selected>Bank Announcement</option>
          <option value="Option 2">Non-Technical Issue Resolution</option>
        </select>
        <h1 class="banker-text07">
          <span>Announcements</span>
          <br />
        </h1>
      </div>
