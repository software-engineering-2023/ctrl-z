            class="banker-textinput3 input"
          />
          <select class="banker-select1">
            <option value="Option 1" selected>Reject</option>
            <option value="Option 2">Accept</option>
          </select>
        </form>
        <button class="banker-button2 button" onclick="showAlert()">Respond</button>
        <script>
          function showAlert(){
            alert("Response sent!");
          }
        </script>
